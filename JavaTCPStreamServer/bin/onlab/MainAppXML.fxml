<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootVBox" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="onlab.MainAppController">
   <children>
      <MenuBar fx:id="menuBar">
        <menus>
          <Menu fx:id="mFile" mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="mExit" mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu fx:id="mHelp" mnemonicParsing="false" text="Help">
            <items>
              <MenuItem fx:id="mAbout" mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</VBox>
